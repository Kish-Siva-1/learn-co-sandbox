<div class="container">
  <div class="col-md-8 col-md-offset-2">
  <h2><%= current_user.username.capitalize %>'s Investments </h2>
  
  <% current_user.portfolios.each do |y| %>
  <h3>  <%=y.name.capitalize%>'s Portfolio </h3>
    <%y.stocks.each do |x|%>
      <%= x.name %>
    <%end%>
      <%= y.stock_weight %>
  <%end%>
  
  </div>
</div>